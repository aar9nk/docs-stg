{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Dovetail",
  "theme": "maple",
  "description": "Wizard of oz",
  "favicon": "/images/branding/favicon.png",
  "logo": {
    "light": "/images/branding/logo/dvtl-logo-fullcolor.svg",
    "dark": "/images/branding/logo/dvtl-logo-reverse.svg"
  },
  "icons": {"library":"lucide"},
  "colors": {
    "primary": "#5550FF",
    "light": "#0C0C15",
    "dark": "#0C0C15"
  },
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Dovetail Docs",
        "icon": "book",
        "description": "Get an overview of Dovetail's features",
        "groups": [
          {
            "group": "New to Dovetail",
            "pages": ["help/new-to-dovetail/welcome-to-dovetail"]
          },
          {
            "group": "Channels",
            "pages": ["help/channels/channels"]
          }
        ]
      },
      {
        "dropdown": "Developers",
        "icon": "code",
        "description": "Developer tools and APIs",
        "tabs": [
          {
            "tab": "Guides",
            "groups": [
              {
                "group": "Getting started",
                "pages": ["api-reference/introduction"]
              }
            ]
          }
          
        ]
      }
    ]
  },
  "global": {
    "anchors": [
      {
        "anchor": "Documentation",
        "href": "https://mintlify.com/docs",
        "icon": "book-open-cover"
      },
      {
        "anchor": "Community",
        "href": "https://mintlify.com/community",
        "icon": "slack"
      },
      {
        "anchor": "Blog",
        "href": "https://mintlify.com/blog",
        "icon": "newspaper"
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "Trust center",
        "href": "mailto:ross@dovetail.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
    "banner": {"content": "ðŸš€ get minted!"},
    "contextual": {"options": ["chatgpt", "claude"]}
}
